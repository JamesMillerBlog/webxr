{
  "name": "@webxr/client",
  "private": "true",
  "_moduleAliases": {
    "@shared": "./../shared"
  },
  "scripts": {
    "start:dev": "NODE_ENV=development NEXT_PUBLIC_LOCAL_IP_ADDRESS=$(node -e \"console.log(require('os').networkInterfaces()[Object.keys(require('os').networkInterfaces())[1]].find(entry => entry.family === 'IPv4').address)\") next dev -p 3001 & npx local-ssl-proxy --source 3000 --target 3001",
    "build": "next build && tsc",
    "lint": "next lint . --ext ts --ext tsx --ext js",
    "lint-and-fix": "next lint . --ext ts --ext tsx --ext js --fix",
    "pulumi:stack:select": "cd infrastructure && pulumi stack select",
    "pulumi:preview": "cd infrastructure && pulumi preview",
    "pulumi:up": "cd infrastructure && pulumi up -y",
    "pulumi:down": "cd infrastructure && pulumi down -y",
    "pulumi:local:up": "cd infrastructure && localstack start & pulumilocal up",
    "pulumi:local:destroy": "cd infrastructure && localstack start & pulumilocal destroy && localstack stop",
    "pulumi:login": "cd ../../ && ts-node ./scripts/pulumi/pulumiStackLogin.ts client",
    "deploy": "npm run pulumi:login && npm run predeploy:generateEnvs && npm run build && npm run pulumi:up && npm run postdeploy:cleanUp",
    "destroy": "npm run pulumi:login && npm run pulumi:down",
    "predeploy:generateEnvs": "cd ../../ && ts-node ./scripts/env/generateDeploymentEnvs.ts client",
    "postdeploy:cleanUp": "cd ../../ && ts-node ./scripts/env/postDeploymentCleanup.ts client"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^5.0.6",
    "@pulumi/aws": "^5.0.0",
    "@pulumi/awsx": "^1.0.0",
    "@pulumi/pulumi": "^3.0.0",
    "@pulumi/synced-folder": "^0.11.1",
    "@react-three/drei": "^9.80.0",
    "@react-three/fiber": "^8.13.6",
    "@react-three/xr": "^5.6.2",
    "aws-sdk": "^2.1445.0",
    "axios": "^1.4.0",
    "babel-plugin-styled-components": "^2.1.4",
    "mime": "^3.0.0",
    "module-alias": "^2.2.3",
    "next": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-websocket": "^4.3.1",
    "styled-components": "^6.0.5",
    "three": "^0.155.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/styled-components": "^5.1.26",
    "@types/three": "^0.154.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "aws-crt": "^1.17.0",
    "encoding": "^0.1.13",
    "eslint": "^8.46.0",
    "eslint-config-next": "^13.4.12",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "3.0.0",
    "typescript": "5.1.6"
  }
}
