{
  "name": "@jamesmillerblog/webxr",
  "private": "true",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "workspaces": [
    "src/client",
    "src/shared",
    "src/server"
  ],
  "dependencies": {
    "@pulumi/aws": "^5.0.0",
    "@pulumi/pulumi": "^3.0.0",
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^5.0.10"
  },
  "scripts": {
    "dev": "npm run start:dev --workspace=@webxr/client & npm run start:dev --workspace=@webxr/server",
    "deploy": "npm run build --workspace=@webxr/client && npm run build:all --workspace=@webxr/server && sleep 5 && npm run pulumi:up",
    "pulumi:login": "npm run pulumi:login --workspace @webxr/shared",
    "pulumi:stack:init": "npm run pulumi:stack:init --workspace @webxr/shared",
    "pulumi:stack:select": "npm run pulumi:stack:select --workspace @webxr/shared",
    "pulumi:preview": "npm run pulumi:preview --workspace @webxr/shared",
    "pulumi:up": "npm run pulumi:up --workspace @webxr/shared",
    "pulumi:down": "npm run pulumi:down --workspace @webxr/shared",
    "pulumi:stack:rm": "npm run pulumi:stack:rm --workspace @webxr/shared",
    "pulumi:whoami": "npm run pulumi:whoami --workspace @webxr/shared",
    "pulumi:local:up": "npm run pulumi:local:up --workspace @webxr/shared",
    "pulumi:local:destroy": "npm run pulumi:local:destroy --workspace @webxr/shared"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesMillerBlog/webxr.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "author": "James Miller",
  "license": "ISC"
}
